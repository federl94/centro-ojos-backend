{
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/centromedico/auth/nuevo",
        "methods": ["POST"],
        "dest": "index.js",
        "headers": {
          "Access-Control-Allow-Origin": "http://localhost:5173",
          "Access-Control-Allow-Methods": "POST",
          "Access-Control-Allow-Headers": "Content-Type"
        }
      },
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }
  